{
	"name": "coming-soon",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "next dev --turbo",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"typecheck": "bun x tsc --noEmit",
		"preview": "bun x serve out",
		"deploy": "bun run build && wrangler pages deploy out --project-name=coming-soon",
		"deploy:prod": "bun run build && wrangler pages deploy out --project-name=coming-soon --branch=main"
	},
	"dependencies": {
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"next": "14.2.5",
		"gsap": "^3.12.5",
		"split-type": "^0.3.4"
	},
	"devDependencies": {
		"wrangler": "^3.99.0",
		"@types/node": "^22",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"typescript": "^5.7",
		"eslint": "^9",
		"eslint-config-next": "14.2.5",
		"tailwindcss": "^3.4.17",
		"postcss": "^8.4.49",
		"autoprefixer": "^10.4.20",
		"tailwindcss-animate": "^1.0.7"
	},
	"engines": {
		"node": ">=20.0.0"
	},
	"packageManager": "bun@1.3.8"
}
